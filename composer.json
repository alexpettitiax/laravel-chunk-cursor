{
    "name": "axterisko/laravel-chunk-cursor",
    "description": "Laravel Eloquent cursor with eager loading and chunking",
    "keywords": [
        "laravel",
        "eloquent"
    ],
    "homepage": "https://github.com/axterisko/laravel-chunk-cursor",
    "license": "MIT",
    "authors": [
        {
            "name": "axterisko",
            "email": "info@axterisko.it",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Itsimiro\\ChunkCursor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itsimiro\\ChunkCursor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itsimiro\\ChunkCursor\\Providers\\LaravelChunkCursorProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
